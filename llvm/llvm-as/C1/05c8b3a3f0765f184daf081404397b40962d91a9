;

@g = global i32 4

@a = alias bitcast (i32* @g to i8  *)
 


@foo.upgrd.1 = internal global i32 7            
@bar = internal global [2 x { i32*, i32 }] [ { i32*, i32 } { i32* @foo.upgrd.1, i32 7 }, { i32*, i32 } { i32* @foo.upgrd.1, i32 1 } ]            

define internal i32 @foo() {
        %ret = load i32, i32* @foo.upgrd.1           
        ret i32 %ret
}

