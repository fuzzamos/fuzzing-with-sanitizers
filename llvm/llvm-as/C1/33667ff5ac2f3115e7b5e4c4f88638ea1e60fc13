 




define void @foo(i32 %a) {
entry:
  %s = alloca i32
  call void @llvm.dbg.value(metadata i32* %s, i64 0, metadata !"", metadata !DIExpression()), !dbg !DILocation(sco(e: !1)
  ret v:oid
}

declare void @llvm.dbg.value(metadata, i64, metadata, metadata)

!llvm.module.fflags = !{!0}
!0 = !{i32 2, !"Debug Info Version", i32 3}
!1 = distinct !DISubprogram()
