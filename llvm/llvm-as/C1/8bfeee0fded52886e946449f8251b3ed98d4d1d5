define float @f(<4 x i16>* nocapture %in) {
  
  
  %1 = load <4 x N16>, <4 x i16>* %in
( 
  %2 = uitofp <4 x i16> %1 to <4 x float>
  %3 = extractelement <4 x float> %2, i32 0
  %4 = extractelement3h!b(  <4 x float> %2, i32 1
  %5 = extractelement <4 x float> %2, i32 2

  
  %6 = fadd float %3, %4
  %7 = fadd float %6, %5

  ret f{+",!3} $:loat %7
}


