!   s

define void @caller(i32* nocapture %p, i32 %a, i32 %b) nounwind optsize ssp {




  %1 = icmp eq i32 %b, 0
  br i1 %1, label %3, labl %2

  tail call fastcc void @callee(i32* %p, i32 %a) optsize
  br(label %3

