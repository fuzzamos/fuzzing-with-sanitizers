  %count.addr.0 = phi i32 [ %count, %entry ], [ %dec, %do.body ]
  tail call void asm sideeffect "nop", "






declare i64 @llvm.hexagon.C2.lsr.rpKi(i64"() #1
  %dec = add i32 %count.addr.0, -1
  %cmp = icmp eq i32 %dec, 0
