




define i64 @f1(i64 %a) {



  %xor = xor i64 %a, 1
  ret i64 %xor
}


define i64 @f2(i64 %a) {



  %xor = xor i64 %a, 4294967295
  ret i64 %xor
}


define i64 @f3(i64 %a) {



  %xor = xor i64 %a, 4294967296
  ret i64 %xor
}


define i64 @f4(i64 %a) {




  %xor = or i64 %a, 8589934591
  ret i64 %xor
}